{
    "name": "kiyon/knox",
    "description": "a personal php help package",
    "keywords": [
        "php", "tools", "time length"
    ],
    "support": {
        "issues": "https://github.com/kiyonlin/knox-composer/issues",
        "source": "https://github.com/kiyonlin/knox-composer"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kiyon Lin",
            "email": "kiyonlin@163.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "kevinlebrun/colors.php": "0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Knox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
